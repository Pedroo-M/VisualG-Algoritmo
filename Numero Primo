algoritmo "NumeroPrimo"
var
   N, C, ContDiv: Inteiro
inicio
      C <- 1
      ContDiv <- 0
      Escreva("Digite um numero: ")
      Leia(N)
      Repita
            Se (C % N = 0) entao
               ContDiv <- ContDiv + 1
            FimSe
            C <- C + 1
      Ate (C > N)
      Se (ContDiv > 2) Entao
         EscrevaL("O valor ", N, " nao e primo!")
      Senao
           EscrevaL("O valor ", N, " e primo!")
      FimSe
fimalgoritmo
