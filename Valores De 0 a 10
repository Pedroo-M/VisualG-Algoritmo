algoritmo "ValoresDe0a10"
var
   C, V, Tot010, Impar: Inteiro
inicio
      Para C <- 1 ate 6 faca
           Escreva("Digite um Valor: ")
           Leia(V)
           Se (V >= 0) e (V <= 10) entao
           Tot010 <- Tot010 + 1
           Fimse
           Se (V % 2 = 1) entao
           Impar <- Impar + V
           FimSe
      FimPara
      EscrevaL("Ao todo foram ", Tot010, " valores entre 0 e 10")
      EscrevaL(" A soma de impares foi: ", Impar)
fimalgoritmo
