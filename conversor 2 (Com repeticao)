 algoritmo "semnome"
 var
    C, Q: Inteiro
    R, D: Real
 inicio
       C <- 1
       Escreva("Quantas vezes vc ira converter? ")
       Leia(Q)
       Enquanto (C <= Q) Faca
                Escreva("Qual o valor? R$")
                Leia(R)
                D <- R/5
                EscrevaL("O valor convertido e US$ ", D:5:2)
                C <- C + 1
       FimEnquanto
 fimalgoritmo
